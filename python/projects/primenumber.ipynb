   ],
   "source": [
    "number = int(input(\"Enter an integer number: \"))\n",
    "control = False\n",
    "if number > 1:\n",
    "      for i in range(2, number):\n",
    "        if number % i == 0:\n",
    "            control = True\n",
    "if control:\n",
    "    print(number, \"is not a prime number\")\n",
    "else:\n",
    "    print(number, \"is a prime number\")"
   ]
